#include <stdio.h>
#include<iostream>
using namespace std;
int knapsack(int wt[],int val[],int c,int n)
{
    if (n==0|| c==0)
    return 0;
    
    if (wt[n-1]<=c)
    return max(val[n-1]+knapsack(wt,val,c-val[n-1],n-1),knapsack(wt,val,c,n-1));
    
    if(wt[n-1]>c)
    return knapsack(wt,val,c,n-1);
    
    
}


int main()
{
    int c,n,i;
    cin>>n>>c;
    int wt[n],val[n];
    for (i=0;i<n;i++)
    cin>>wt[i];
    for (i=0;i<n;i++)
    cin>>val[i];
    
    cout<<knapsack(wt,val,c,n);
    return 0;
}
