
#include <stdio.h>
#include<iostream>
using namespace std;
bool equal(int val[],int s,int n)
{
    if(s==0)
    return true;
    if (n==0)
    return false;
    
    else if(val[n-1]<=s)
    return equal(val,s-val[n-1],n-1)||equal(val,s,n-1);
    
    else if(val[n-1]>s)
    return equal(val,s,n-1);
}


int main()
{
    int n,i,s=0;
    cin>>n;
    int val[n];
    for (i=0;i<n;i++)
    cin>>val[i];
    
    for(i=0;i<n;i++)
    s+=val[i];
    
    if(s%2!=0)
    cout<<false;
    
    else
    cout<<equal(val,s/2,n);
    return 0;
}
